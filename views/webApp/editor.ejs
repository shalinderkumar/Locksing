	<script>
		$('.editor .pdf-header .search-bar input').on('click',function(){			
			$('.editor .suggestion').show();
			$(this).addClass('toggle');
		});
		$(document).click(function(){
			$('.editor .suggestion').hide();
			$('.editor .pdf-header .search-bar input').removeClass('toggle');
		});
		$('.editor .suggestion,.editor .pdf-header .search-bar input').click(function(e){
		    e.stopPropagation();
		});

		var header_h = $('.pdf-header').innerHeight();
		$('.pdf-container').css("margin-top",header_h);

		

		$('.editor .pdf-container .pdf-tools ul li a').click(function(){
			$(this).parent('li').siblings('li').find('a').removeClass('active');
			$(this).addClass('active');
		});


		$('.toggle-btn').click(function(){
			$('.pdf-container').addClass('slide-box');

		});
		$('.uncompress-btn a').click(function(){
			$('.pdf-container').removeClass('slide-box');

		});
		
		$('.uncompress-btn').css("top",header_h + 25);
		$('.brief-pages,.pdf-container .right-section').css('margin-top',header_h);

		    /* My tags*/
	     $('#myTags').tagit();
		   
	    /*dragable*/
	     $( ".drag-area-outer" ).draggable({
	     	containment: ".pdf-viewer" 
	     });
	      $( ".drag-area" ).resizable({
	      	containment: ".pdf-viewer",
	      	// animate: true,
	      	// animateDuration: "fast"
	      });

	     /*editor*/

	     $('.text-box').click(function(){	     	
	     	$('.editor .pdf-viewer .pdf-addition .name').find('.drag-area').show();
	     	$('.editor .pdf-viewer .pdf-addition .name').find('.drag-area textarea').focus();
	     });
	     $('.date-box').click(function(){	     	
	     	$('.editor .pdf-viewer .pdf-addition .date').find('.drag-area').show();
	     });
	     $('.show-signature').click(function(){	     	
	     	$('.editor .pdf-viewer .pdf-addition .signature').find('.drag-area').show();
	     });


	     	$( "#tabs" ).tabs();
	     	$( "#tabs1" ).tabs();
	     	$( "#tabs-2" ).tabs();
