                resizeCanvas();
            });


		    	$('.button-circle').click(function(){
					$('.button-circle').removeClass('active');
					$(this).addClass('active');
				});

			// text  change signature
			    function change_text() {
			        var text = $('#name').val();
			        $('.text_style').html(text);
			    }
			



			/*format functions*/

			$('.font-dec').click(function(){
				var change_size = $(this).parent('.edit-tools').siblings('.editable').find('.change_size');
				var font_dec =  parseInt($(this).parent('.edit-tools').siblings('.editable').find('.change_size').css("font-size"));		
				font_dec = font_dec - 1 + "px";
				change_size.css("font-size",font_dec);

			});
			$('.font-inc').click(function(){
				var change_size = $(this).parent('.edit-tools').siblings('.editable').find('.change_size');
				var font_dec =  parseInt($(this).parent('.edit-tools').siblings('.editable').find('.change_size').css("font-size"));		
				font_dec = font_dec + 1 + "px";
				change_size.css("font-size",font_dec);
			});

			$('.colorBtn input').click(function() {	
				var change_size = $(this).parents('.edit-tools').siblings('.editable').find('.change_size');		
				if ($('.colorBtn input:checked').val()){
					var color1 = $(this).val();			
					change_size.css({ "color": color1 });
					
				}
			});
			
			$('.delete-btn').click(function() {
				$(this).parents('.drag-area-outer').remove();
			});
			if ($(window).width() < 768) {
				$('.pdf-container').addClass('slide-box');
			}
			

			$('.page-count a').click(function(){
				$('.pdf-container').addClass('slide-box2');
			});
			$('.toggle-btn2').click(function(){
				$('.pdf-container').removeClass('slide-box2');
			});

			$(document).click(function(){
				$('.pdf-container').removeClass('slide-box2');
			});
			$('.toggle-btn2,.page-count a').click(function(e){
		    e.stopPropagation();
			});
			
	</script>

